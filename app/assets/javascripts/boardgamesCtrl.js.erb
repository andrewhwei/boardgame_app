(function () {
  "use strict";

  angular.module("app").controller("boardgamesCtrl", ['$scope', '$http', function($scope, $http) {

  $scope.setup = function() {
    $http.get("/api/v1/boardgames.json").then(function(response) {
      $scope.boardgames = response.data;
    });
  }

  window.$scope = $scope;
  }]);
})();